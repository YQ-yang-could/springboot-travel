<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yang.travel.mapper.TravellerMapper">

    <select id="findByOrderId" resultType="com.yang.travel.pojo.Traveller">
        select * from traveller where id in(select travellerId from order_traveller where orderId=#{orderId});
    </select>
</mapper>